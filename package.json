{
  "name": "spec-kit-project-test",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"npm run dev:client\" \"npm run dev:server\"",
    "dev:client": "vite",
    "dev:server": "nodemon server/server.ts",
    "build": "tsc -b && vite build",
    "build:server": "tsc -p server/tsconfig.json",
    "start": "npm run build && npm run build:server && npm run start:server",
    "start:server": "node server/dist/server.js",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx",
    "lint:fix": "eslint . --ext .ts,.tsx,.js,.jsx --fix",
    "type-check": "tsc --noEmit",
    "preview": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --environment jsdom",
    "spec:pack": "node .spec/scripts/pack-context.js",
    "spec:gen": "uvx --from git+https://github.com/github/spec-kit.git specify tasks --format json > .spec/_gen_tasks.json",
    "spec:diff": "diff -u .spec/tasks.json .spec/_gen_tasks.json || (echo '\\n‚ùå Spec tasks changed. Run `npm run spec:update` if you INTEND to accept the changes.' && exit 1)",
    "spec:update": "mv .spec/_gen_tasks.json .spec/tasks.json",
    "ai:pack": "node .ai/bin/pack-context.js"
  },
  "dependencies": {
    "@mantine/core": "^7.14.2",
    "@mantine/form": "^7.14.2",
    "@mantine/hooks": "^7.14.2",
    "@mantine/modals": "^7.14.2",
    "@mantine/notifications": "^7.14.2",
    "@tabler/icons-react": "^3.27.0",
    "cors": "^2.8.5",
    "express": "^5.1.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.33.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.2.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/node": "^24.5.1",
    "@types/react": "^19.1.10",
    "@types/react-dom": "^19.1.7",
    "@typescript-eslint/eslint-plugin": "^8.44.0",
    "@typescript-eslint/parser": "^8.44.0",
    "@vitejs/plugin-react": "^5.0.0",
    "concurrently": "^9.2.1",
    "eslint": "^9.33.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.3.0",
    "jsdom": "^25.0.1",
    "nodemon": "^3.1.10",
    "tsx": "^4.20.5",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.39.1",
    "vite": "^7.1.2",
    "vitest": "^2.1.5"
  }
}
